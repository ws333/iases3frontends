{
  "name": "docs",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "pnpm --filter webapp-frontend --filter docs run dev:vite",
    "dev:vite": "vite",
    "build": "vite build",
