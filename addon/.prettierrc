{
    "trailingComma": "es5",
    "tabWidth": 4,
    "semi": true,
    "singleQuote": false,
    "printWidth": 120,
    "plugins": ["@trivago/prettier-plugin-sort-imports"],
    "importOrder": [
        "<THIRD_PARTY_MODULES>",
        "/types/(.*)$",
        "/constants/(.*)$",
        "/components/(.*)$",
        "/hooks/(.*)$",
        "\/helpers[^\/]*\/[^\/]+$",
        "^[./](?!.*\\.css$)",
        ".*\\.css$"
    ],
    "importOrderSeparation": false,
    "importOrderSortSpecifiers": true
}
