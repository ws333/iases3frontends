{
  "name": "webapp-docs",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "pnpm --filter webapp-frontend --filter webapp-docs run dev:vite",
    "dev:vite": "vite",
    "build": "vite build",
    "check": "tsc --noemit",
    "preview": "vite preview",
    "format": "npx prettier --write .",
    "format:check": "npx prettier --check .",
    "lint": "npx eslint .",
    "ca": "pnpm run check && pnpm run lint && pnpm run format:check"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^6.30.1",
    "rehype-raw": "^7.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "typescript": "^5.8.3"
  }
}
