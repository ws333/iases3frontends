{
  "name": "webapp",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "pnpm --filter webapp-frontend --filter webapp-docs run dev:vite",
    "build": "pnpm --filter webapp-frontend --filter webapp-docs run build",
    "upload": "pnpm --filter webapp-docs run build && cd frontend && pnpm run upload",
    "check": "pnpm --filter webapp-frontend --filter webapp-docs run check",
    "format": "cd frontend && pnpm run format",
    "format:check": "cd frontend && pnpm run format:check",
    "lint": "pnpm --filter webapp-frontend --filter webapp-docs run lint",
    "ca": "pnpm --filter webapp-frontend --filter webapp-docs run ca"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "eslint": "^9.21.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "prettier": "^3.5.2",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.24.1"
  }
}
