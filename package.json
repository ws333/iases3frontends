{
  "name": "iases3frontends",
  "version": "1.0.0",
  "private": true,
  "description": "IASES3 unified workspace for frontends",
  "scripts": {
    "build": "pnpm -r build",
    "dev:addon": "cd addon/packages/browser-preview && pnpm run dev",
    "dev:addon:watch": "cd addon/packages/interface && pnpm run watch",
    "dev:webapp": "cd webapp/frontend && pnpm run dev",
    "build:addon": "cd addon && pnpm run build",
    "build:webapp": "cd webapp/frontend && pnpm run build",
    "check": "pnpm -r check",
    "format": "pnpm -r format",
    "format:check": "pnpm -r format:check",
    "lint": "pnpm -r lint",
    "ca": "pnpm -r ca",
    "clean": "pnpm -r clean && ./scripts/clean_and_pnpm.sh"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.2",
    "eslint": "^9.35.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "pnpm": "^10.15.1",
    "prettier": "^3.6.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.43.0",
    "vite": "^7.3.0",
    "vitest": "^4.0.16"
  }
}
